<!ELEMENT scores (predictor*)>

<!ENTITY % predictor.content "EMPTY">
<!ENTITY % predictor.coordinates "index NMTOKEN #REQUIRED">
<!ENTITY % predictor.extra-attrs "score CDATA #REQUIRED">

<!ELEMENT predictor %predictor.content;>
<!ATTLIST predictor
  index NMTOKEN #REQUIRED
  scores CDATA #REQUIRED
  stat1 CDATA #REQUIRED
  stat2 CDATA #REQUIRED
  %predictor.coordinates;
  %predictor.extra-attrs;
>
